# Wmoge asset meta file
version: 1
uuid: 0
loader: AssimpMeshAssetLoader
rtti: Mesh
deps: [ ]
description: "suzanne test mesh"
import_data:
  rtti: AssimpMeshImportData
  source_files: [{file: "assets/mesh/suzanne.obj"}]
  attributes: [ "Pos3f", "Norm3f" ]
  process:
    triangulate: true
    tangent_space: true
    flip_uv: true
    gen_normals: false
    gen_smooth_normals: true
    join_identical_vertices: true
    limit_bone_weights: true
    improve_cache_locality: true
    sort_by_ptype: true
    gen_uv: true


